\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

master@3*1
\end_layout

\begin_layout Plain Layout

diamond[1,0,1,1,0]*-1
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

master@4*1
\end_layout

\begin_layout Plain Layout

diamond[1,-1,1,1,0]*-1
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

master@5*1
\end_layout

\begin_layout Plain Layout

